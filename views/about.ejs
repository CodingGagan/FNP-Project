<%- include('./partials/header'); %>
<div class="dashboard">
    <div class="icon">
        <i class='bx bxl-xing'></i>
        <div class="icon1">
            <a href=" ">View Site<i class='bx bx-chevron-down'></i> </a>
            <a href=" ">Log out </a>
        </div>

    </div>
        <div class="dash">
            <div class="home">
                <div class="hp">
                    <h2>Contact Us</h2>
                <p>Home / Contact us</p>
                </div>
                <div class="hp-1">
                    <a id="Admin" href=""><i class="fa-solid fa-user"></i> Admin</a>
                   
                </div>
            </div>
            <div class="all">
                <h2>Contact Us</h2>
            </div>
            <div class="select-box-wrap">
                <select class="form-select" aria-label="Default select example">
                    <option selected>Bulk option</option>
                    <option>Delete</option>
                </select>
                <button type="button" class="btn btn-primary">Apply</button>
                <!-- <a href="/adddetails" class="btn btn-primary" style="margin-left: 1000px;">Add </a> -->

            </div>
            <div class="row">
                <dov class="col-12">
                    <table id="example" class="display" style="width:100%; margin-top: 15px;">
                        <thead style="background-color: #0A2558; color:white;">
                                <tr>
                                        <td><input type="checkbox" style="margin-left: 15px; "></t>
                                        
                                        <td>Title</td>
                                        <td>Value</td>
                                       
                                       
                                </tr>
                        </thead >
                        <tbody style="padding: 10px;">

                            <%
                            if(sampleData.length >0){
                                sampleData.forEach(function(data){                                  
                                    %>
                                
                                    <tr>
                                        <td><input type="checkbox" style="margin-left: 15px; "></t>
                                       <td>Email</td>
                                      
                                        <td style="padding: 10px;">  <%=data.email %>  </td>
                                    
                                    </tr>
                                    <tr>
                                        <td><input type="checkbox" style="margin-left: 15px; "></t>
                                       <td>Address</td>
                                      
                                        <td style="padding: 10px;">  <%=data.address %>  </td>
                                    
                                    </tr>
                                    <tr>
                                        <td><input type="checkbox" style="margin-left: 15px; "></t>
                                       <td>Phone no </td>
                                      
                                        <td style="padding: 10px;">  <%=data.phoneno %>  </td>
                                    
                                    </tr>
                                    <tr>
                                        <td><input type="checkbox" style="margin-left: 15px; "></t>
                                       <td>Whatsapp no </td>
                                      
                                        <td style="padding: 10px;">  <%=data.whatsapp %>  </td>
                                    
                                    </tr>
                                    <tr>
                                        <td><input type="checkbox" style="margin-left: 15px; "></t>
                                       <td>Website Title </td>
                                      
                                        <td style="padding: 10px;">  <%=data.website %>  </td>
                                    
                                    </tr>
                                    <tr>
                                        <td><input type="checkbox" style="margin-left: 15px; "></t>
                                       <td>Facebook Details </td>
                                      
                                        <td style="padding: 10px;">  <%=data.social %>  </td>
                                    
                                    </tr>
                                    <tr>
                                        <td><input type="checkbox" style="margin-left: 15px; "></t>
                                       <td>Instagram Details </td>
                                      
                                        <td style="padding: 10px;">  <%=data.insta %>  </td>
                                    
                                    </tr>
                                    <tr>
                                        <td><input type="checkbox" style="margin-left: 15px; "></t>
                                       <td>Website Image </td>
                                      
                                        <td style="padding: 10px;"> <img src="<%=data.wimage %> " style="height: 100px ;width: 100px; " alt="image"> </td>
                                    
                                    </tr>
                                    <tr>
                                        <td><input type="checkbox" style="margin-left: 15px; "></t>
                                       <td>Logo </td>
                                      
                                        <td style="padding: 10px;">  <img src="<%=data.logo %> " style="height: 100px ;width: 100px; " alt="image">  </td>
                                    
                                    </tr>
                                    <tr>
                                        <td>

                                            <a href="/adddetails/edit/<%= data.id %>"  class="btn btn-primary" ><i class='bx bxs-edit-alt'>Update Status</i></a>
                                        </td>
                                    </tr>
                                        
                                    <%
                                })
                            } 
                            
                            else{
                            %>
                                <tr>
                                    
                                    <td colspan="5">No Data Found</td>
                                </tr>
                            %> 
                            <%   
                            }
                            %>  
                                               
                        </tbody>

                    </table>
                        
                   
                       
                    
            </div>


                
        </div>
</div>
           
<%- include('./partials/footer'); %>
